{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "server_v1",
      "image": "170360853218.dkr.ecr.ap-northeast-2.amazonaws.com/server_v1",
      "hostname": "server1",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server_v2",
      "image": "170360853218.dkr.ecr.ap-northeast-2.amazonaws.com/server_v2",
      "hostname": "server2",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "170360853218.dkr.ecr.ap-northeast-2.amazonaws.com/nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["server_v1", "server_v2"],
      "memory": 128
    }
  ]
}
